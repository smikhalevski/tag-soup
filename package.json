{
  "name": "tag-soup",
  "version": "2.0.0",
  "description": "The fastest pure JS SAX/DOM XML/HTML parser.",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "vitest run",
    "perf": "NODE_OPTIONS=--expose-gc toofast",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/tag-soup.git"
  },
  "keywords": [
    "tiny",
    "forgiving",
    "stream",
    "fast",
    "sax",
    "dom",
    "html",
    "xml",
    "parser"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/tag-soup/issues"
  },
  "homepage": "https://github.com/smikhalevski/tag-soup#readme",
  "devDependencies": {
    "@types/node": "^24.7.0",
    "htmlparser-benchmark": "^1.1.3",
    "htmlparser2": "^10.0.0",
    "hyntax": "^1.1.9",
    "parse5": "^8.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "toofast": "^3.0.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.13",
    "typedoc-plugin-mdn-links": "^5.0.9",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "flyweight-dom": "^2.2.1",
    "speedy-entities": "^3.1.0"
  }
}
