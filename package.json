{
  "name": "tag-soup",
  "version": "1.1.1",
  "description": "The fastest pure JS SAX/DOM XML/HTML parser.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run gen:rule-iterator && rollup --config --configPlugin typescript",
    "clean": "rimraf ./src/main/gen ./lib ./docs",
    "test": "jest",
    "perf": "[ -d ./lib ] || npm run build && node --expose-gc --max-old-space-size=4096 ./node_modules/.bin/toofast ./src/test/perf.js",
    "docs": "typedoc --name 'TagSoup üçú' --readme none ./src/main/index.ts",
    "gen:rule-iterator": "ts-node ./src/scripts/gen-rule-iterator.ts ./src/main/gen/ruleIterator.ts && prettier --write ./src/main/gen/ruleIterator.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/tag-soup.git"
  },
  "keywords": [
    "tiny",
    "forgiving",
    "stream",
    "fast",
    "sax",
    "dom",
    "html",
    "xml",
    "parser"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/tag-soup/issues"
  },
  "homepage": "https://github.com/smikhalevski/tag-soup#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "htmlparser-benchmark": "^1.1.3",
    "htmlparser2": "^8.0.2",
    "hyntax": "^1.1.9",
    "jest": "^29.5.0",
    "parse5": "^7.1.2",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "sax": "^1.2.4",
    "toofast": "^2.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "flyweight-dom": "^1.2.0",
    "speedy-entities": "^3.0.1",
    "tokenizer-dsl": "^5.0.0"
  }
}
